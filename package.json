{
  "name": "repair-shop-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start-web-only.js",
    "dev:all": "expo start",
    "dev:web": "expo start --web",
    "dev:clear": "expo start --clear",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "quality:check": "node scripts/quality-check.js check",
    "quality:fix": "node scripts/quality-check.js fix",
    "quality:report": "node scripts/quality-check.js report",
    "quality:metrics": "node scripts/quality-metrics.js collect",
    "quality:metrics:report": "node scripts/quality-metrics.js report",
    "organize-imports": "node scripts/organize-imports.js",
    "pre-commit": "npm run quality:check",
    "audit:security": "npm audit --audit-level=moderate",
    "fix:dev": "node scripts/fix-dev-server.js fix",
    "clear:cache": "node scripts/fix-dev-server.js clear",
    "install:android": "powershell -ExecutionPolicy Bypass -File scripts/install-android-sdk.ps1",
    "build:apk": "node scripts/build-apk.js",
    "build:android": "eas build --platform android --profile preview",
    "build:production": "eas build --platform android --profile production",
    "fix:assets": "node scripts/fix-assets.js",
    "phone": "node scripts/start-for-phone.js",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "54",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.475.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.4",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
